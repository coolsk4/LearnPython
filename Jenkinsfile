        echo 'currently in Build stage'
      }      
    }  
    
    stage("test"){
      
      steps{
        echo 'currently in test stage'
      }      
    }  
    
    stage("run"){
      
      steps{
        echo 'currently in run stage'
      }      
    }  
  }  
}
